usCritico = {\r\n    'background-color': 'rgb(253, 193, 0)',\r\n    'box-shadow': '0px 0px 12px 2px rgb(253, 193, 0)'\r\n  }\r\n  const statusVulneravel = {\r\n    'background-color': 'rgb(255, 129, 84)',\r\n    'box-shadow': '0px 0px 12px 2px rgb(255, 129, 84)'\r\n  }\r\n\r\nreturn (\r\n  <div className=\"card-item-container\">\r\n    <div className=\"pane-card\">\r\n      <div className=\"photo-card-shape\">\r\n        <img src={props.imagem} onError={(e)=>fallBackImage(e, props.nome)}></img>\r\n        {imageNotFound && <div style={biomaColor(props.bioma)} className=\"cut-words\">{cutName}</div>}\r\n      </div>\r\n      <div className=\"content-card\">\r\n        <h1>{props.nome}</h1>\r\n        <h2>{props.nomeCientifico}</h2> \r\n        <h3>{props.especie}</h3>\r\n        <div className=\"text-bioma\" style={biomaColor(props.bioma)}>{props.bioma}</div>\r\n      </div>\r\n        <div className=\"status-card\" style={statusColor(props.risco)}></div>\r\n    </div>\r\n  </div>\r\n);\r\n}\r\n\r\nexport default CardMap;\r\n","C:\\Users\\user\\Desktop\\react-aulas\\teste-visual-mix\\src\\components\\Header.js",["43","44"],"import '../styles/Header.css'\r\nimport React, { useState } from 'react';\r\n\r\nfunction Header() {\r\nreturn (\r\n  <div className=\"header-container\">\r\n      <div className=\"shape-img-profile\">\r\n        <img src='avatar.png'></img>\r\n      </div>\r\n  </div>\r\n);\r\n}\r\nexport default Header;\r\n","C:\\Users\\user\\Desktop\\react-aulas\\teste-visual-mix\\src\\components\\Footer.js",["45"],"import '../styles/Footer.css'\r\nimport React, { useState } from 'react';\r\n\r\nfunction Footer() {\r\nreturn (\r\n  <div className=\"footer-container\">\r\n      <footer>Criado por Wesley da Silva Lira/E-mail: wesleyliramtx@gmail.com - 11950313436</footer>\r\n  </div>\r\n);\r\n}\r\nexport default Footer;\r\n",{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","severity":1,"message":"51","line":2,"column":17,"nodeTyp